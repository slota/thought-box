<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :email_address %><br>
    <%= f.text_field :email_address %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.text_area :password %>
  </p>
  <p>
    <%= f.label :password_confirmation %><br>
    <%= f.text_area :password_confirmation %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
